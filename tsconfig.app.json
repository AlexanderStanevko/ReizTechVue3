{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "public/*": [
        "public/*"
      ],
      "assets/*": [
        "src/assets/*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "types/*": [
        "src/types/*"
      ],
      "types": [
        "src/types/index.ts"
      ],
      "pages/*": [
        "src/pages/*"
      ],
      "router/*": [
        "src/router/*"
      ],
      "stores/*": [
        "src/stores/*"
      ],
      "utils/*": [
        "src/utils/*"
      ]
    }
  }
}